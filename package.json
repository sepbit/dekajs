{
    "name": "@vitorteccom/dekajs",
    "version": "6.0.0",
    "description": "This is a simple library to convert Dekatrian and Gregorian.",
    "keywords": [
        "Vitortec.com",
        "converter",
        "date",
        "calendar",
        "dekatrian",
        "gregorian"
    ],
    "homepage": "https://github.com/vitorteccom/dekajs",
    "bugs": {
        "email": "contato@vitortec.com",
        "url": "https://github.com/vitorteccom/dekajs/issues"
    },
    "license": "GPL-3.0-or-later",
    "author": [
        {
            "name": "Vitor Guia",
            "email": "vitor.guia@vitortec.com",
            "url": "https://github.com/vitoranguia"
        }
    ],
    "main": "lib/main",
    "repository": {
        "type": "git",
        "url": "https://github.com/vitorteccom/dekajs.git"
    },
    "scripts": {
        "doc": "jsdoc --configure jsdoc.json",
        "fix-style": "standard --fix 'lib/*.mjs' 'test/*.mjs'",
        "check-style": "standard 'lib/*.mjs' 'test/*.mjs'",
        "test": "nyc --extension .mjs --reporter=html --reporter=text ava 'test/*.mjs'"
    },
    "ava": {
        "require": [
            "esm"
        ],
        "babel": false,
        "extensions": [
            "js",
            "mjs"
        ]
    },
    "dependencies": {
        "esm": "^3.2"
    },
    "devDependencies": {
        "jsdoc": "^3.5",
        "standard": "^12.0",
        "ava": "^1.3",
        "nyc": "^13.3"
    },
    "engines": {
        "node": "^8.11"
    }
}
